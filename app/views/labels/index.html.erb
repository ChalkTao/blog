<h1>Listing Labels</h1>
  <h2>
    <% @labels.each do |label| %>
        <span class="label label-default" style="margin-right: 1rem">
          <%= label.name %> 
          <%= link_to 'x', label, method: :delete %>
        </span>
    <% end %>
  </h2>

<br>

<%= link_to 'New Label', new_label_path, :class => 'btn btn-primary'  %>
